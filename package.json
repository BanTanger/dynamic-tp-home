{
  "name": "dynamic-tp-site",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "bootstrap": "pnpm i",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:lint": "prettier --check --write docs/",
    "docs:clean-dev": "vuepress dev docs --no-cache",
    "packages:check-update": "pnpm dlx npm-check-updates -u --deep --timeout 600000",
    "packages:update": "pnpm dlx vp-update"
  },
  "dependencies": {
    "@vuepress/client": "2.0.0-beta.62",
    "@vuepress/plugin-docsearch": "2.0.0-beta.62",
    "@vuepress/utils": "2.0.0-beta.62",
    "vue": "^3.3.4",
    "vuepress": "2.0.0-beta.62",
    "vuepress-theme-hope": "2.0.0-beta.220"
  },
  "devDependencies": {
    "prettier": "2.8.4",
    "vuepress-plugin-search-pro": "2.0.0-beta.211"
  },
  "packageManager": "pnpm@7.29.1",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
}